<hr>
<div class="recent-search row">
    {% if recent_search|length %}
    <div class="span5">
        <h3>Recent Search</h3>
        <div>
            {% for item in recent_search %}
            <div class="recent-search-item {% if (loop.index + 1) % 2 %}recent-search-item-even{% endif %}">
                <p>
                    <span>
                        <span>From</span>
                        <i class="icon-large icon-google-maps"></i>
                    </span>
                    {{ item.from_name }}
                </p>
                <p>
                    <span>
                        <span>To</span>
                        <i class="icon-large icon-google-maps"></i>
                    </span>
                    {{ item.to_name }}
                </p>
                <p>
                    <span>
                        <span>Time</span>
                        <i class="icon-large icon-clock"></i>
                    </span>
                    {{ item.start_time|date('Y-m-d H:i') }}
                    <button type="button" class="btn btn-info btn-small" onclick="window.location = '{{ url('csg_route_finder.default.index') ~ '?' ~ item.url }}'">View Search</button>
                </p>
            </div>
            {% endfor %}
        </div>
    </div>
    {% endif %}
    
